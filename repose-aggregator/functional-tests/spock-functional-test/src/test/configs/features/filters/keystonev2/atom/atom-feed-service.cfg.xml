<?xml version="1.1" encoding="UTF-8"?>

<atom-feed-service xmlns="http://docs.openrepose.org/repose/atom-feed-service/v1.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <feed id="non-authenticating-feed" uri="http://localhost:${atomPort2}/feed"/>

    <feed id="authenticating-feed" uri="http://localhost:${atomPort}/feed">
        <authentication xsi:type="OpenStackIdentityAuthenticationType"
                        uri="http://localhost:${identityPort}"
                        username="admin_username"
                        password="admin_password"/>
    </feed>
</atom-feed-service>